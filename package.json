{
	"name": "adb-filesystem",
	"displayName": "ADB File System",
	"description": "file system for connected android devices, using adb",
	"version": "0.0.4",
	"publisher": "MakotoHamanaka",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:adbfs.workspaceInit",
		"onFileSystem:adbfs"
	],
	"main": "./out/extension.js",
	"author": {
		"name": "Makoto Hamanaka",
		"email": "ham.lua@gmail.com"
	},
	"license": "MIT",
	"contributes": {
		"commands": [
			{
				"command": "adbfs.workspaceInit",
				"title": "Setup Android Device Files Workspace",
				"category": "AdbFS"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "adbfs.workspaceInit",
					"when": "workbenchState != workspace"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^10.0.10",
		"@types/node": "^18.15.11",
		"@types/vscode": "^1.73.0",
		"eslint": "^9.17.0",
		"glob": "^7.1.4",
		"mocha": "^10.0.0",
		"typescript": "^5.0.2",
		"typescript-eslint": "^8.16.0",
		"vscode-test": "^1.6.1",
		"webpack-cli": "^6.0.1"
	},
	"dependencies": {
		"@devicefarmer/adbkit": "file:../devicefarmer_adbkit",
		"bluebird": "^3.7.0",
		"concat-stream": "^2.0.0",
		"streamifier": "^0.1.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mhama/vscode-adb-filesystem.git"
	}
}
