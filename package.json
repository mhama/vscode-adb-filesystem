{
	"name": "adb-filesystem",
	"displayName": "ADB File System",
	"description": "file system for connected android devices, using adb",
	"version": "0.0.3",
	"publisher": "MakotoHamanaka",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:adbfs.workspaceInit",
		"onFileSystem:adbfs"
	],
	"main": "./out/extension.js",
	"author": {
		"name": "Makoto Hamanaka",
		"email": "ham.lua@gmail.com"
	},
	"license": "MIT",
	"contributes": {
		"commands": [
			{
				"command": "adbfs.workspaceInit",
				"title": "Setup Android Device Files Workspace",
				"category": "AdbFS"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "adbfs.workspaceInit",
					"when": "workbenchState != workspace"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"adbkit": "github:mhama/adbkit",
		"concat-stream": "^2.0.0",
		"streamifier": "^0.1.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mhama/vscode-adb-filesystem.git"
	}
}
